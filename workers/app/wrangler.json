{
  "compatibility_date": "2024-12-05",
  "compatibility_flags": ["nodejs_compat_v2"],
  "main": "./src/index.tsx",

  "name": "unpkg-app",
  "assets": { "directory": "./public" },
  "routes": [{ "pattern": "app.unpkg.com", "custom_domain": true }],
  "services": [{ "binding": "WWW", "service": "unpkg-www" }],
  "vars": {
    "ASSETS_ORIGIN": "https://app.unpkg.com",
    "HOST": "app.unpkg.com",
    "MODE": "production",
    "WWW_ORIGIN": "https://unpkg.com"
  },

  "env": {
    "dev": {
      "name": "unpkg-app-local",
      "routes": [],
      "services": [{ "binding": "WWW", "service": "unpkg-www-local" }],
      "vars": {
        "ASSETS_ORIGIN": "http://localhost:8001",
        "HOST": "localhost:3001",
        "MODE": "development",
        "WWW_ORIGIN": "http://localhost:3000"
      }
    },

    "staging": {
      "name": "unpkg-app-dev",
      "observability": { "enabled": true },
      "routes": [{ "pattern": "app.unpkg.dev", "custom_domain": true }],
      "services": [{ "binding": "WWW", "service": "unpkg-www-dev" }],
      "vars": {
        "ASSETS_ORIGIN": "https://app.unpkg.dev",
        "HOST": "app.unpkg.dev",
        "MODE": "staging",
        "WWW_ORIGIN": "https://unpkg.dev"
      }
    }
  }
}
